{
  "name": "treecraaft",
  "displayName": "TreeCraft - Project Structure Visualizer",
  "description": "Generate beautiful visual representations of your project's folder structure with file-type icons",
  "publisher": "AbdullahMia",
  "icon": "images/logo.png",
  "version": "0.0.4",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/abdullahmia/treecraft"
  },
  "bugs": {
    "url": "https://github.com/abdullahmia/treecraft/issues"
  },
  "keywords": [
    "file-tree",
    "treecraft",
    "treecraaft",
    "tree",
    "folder",
    "structure",
    "visualizer",
    "project",
    "directory",
    "file-icons",
    "export",
    "gitignore"
  ],
  "categories": [
    "Visualization",
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "folderTreeGenerator.generateTree",
        "title": "Generate Tree (Text)",
        "category": "Folder Tree"
      },
      {
        "command": "folderTreeGenerator.generateInteractiveTree",
        "title": "Generate Interactive Tree",
        "category": "Folder Tree"
      },
      {
        "command": "folderTreeGenerator.generateTreeToFile",
        "title": "Generate Tree to File",
        "category": "Folder Tree"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "folderTreeGenerator.generateTree",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace"
        },
        {
          "command": "folderTreeGenerator.generateInteractiveTree",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace"
        },
        {
          "command": "folderTreeGenerator.generateTreeToFile",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace"
        }
      ],
      "commandPalette": [
        {
          "command": "folderTreeGenerator.generateTree"
        },
        {
          "command": "folderTreeGenerator.generateInteractiveTree"
        },
        {
          "command": "folderTreeGenerator.generateTreeToFile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",

    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "canvas": "^2.11.2"
  }
}
