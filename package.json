{
  "name": "treecraft",
  "displayName": "TreeCraft - Project Structure Visualizer",
  "description": "Generate beautiful visual representations of your project's folder structure with file-type icons",
  "publisher": "Abdullah Mia",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "folderTreeGenerator.generateTree",
        "title": "TreeCraft: Generate Tree (Text View)",
        "category": "TreeCraft"
      },
      {
        "command": "folderTreeGenerator.generateTreeToFile",
        "title": "TreeCraft: Generate Tree File",
        "category": "TreeCraft"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "folderTreeGenerator.generateTree",
          "group": "navigation"
        },
        {
          "command": "folderTreeGenerator.generateTreeToFile",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "folderTreeGenerator.generateTree"
        },
        {
          "command": "folderTreeGenerator.generateTreeToFile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "canvas": "^2.11.2"
  }
}
